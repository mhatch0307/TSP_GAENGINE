<BorderPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="tsp.views.TSPDisplayController">
	<center>
		<VBox>
			<HBox alignment = "CENTER" spacing ="10">
				<Label text = "Description:" fx:id="gaDescription" />
			</HBox>
			<HBox alignment = "CENTER" spacing ="10">
				<Label text = "Generation #:" fx:id="generationNumber" />
			</HBox>
			<HBox alignment = "CENTER" spacing ="10">
				<Label text = "Chromosome:" fx:id="chromosome" />
			</HBox>
			<HBox alignment = "CENTER" spacing ="10">
				<Label text = "Fitness Score:" fx:id="fitnessScore" />
			</HBox>
		</VBox>
	</center>
</BorderPane>

